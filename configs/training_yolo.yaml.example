paths:
  data_yaml: "/path/to/your/data/data.yaml"
  runs_dir: "/path/to/your/runs"

model:
  weights: "yolo11n.pt"
  imgsz: 640

training:
  epochs: 50
  batch: 16
  patience: 15
  save_period: 10

augmentation:
  hsv_h: 0.015
  hsv_s: 0.7
  hsv_v: 0.4
  degrees: 0.0
  translate: 0.1
  scale: 0.5
  flipud: 0.0
  fliplr: 0.5
  mosaic: 1.0